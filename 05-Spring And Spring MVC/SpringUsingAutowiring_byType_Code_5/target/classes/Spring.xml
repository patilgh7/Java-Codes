<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<!-- <bean/> definitions here -->

	<!-- Setter Based Injection -->
	
	<!-- autowire="byName" map karto Employee class chya Address add123; la  
	
	<bean class="Address"> Employee class Address =Address in bean  map karto -->

	<bean id="e" class="Employee" autowire="byType">
		<property name="id" value="101"></property>
		<property name="name" value="Ram"></property>
		<property name="salary" value="10000"></property>
		
	</bean>

	<bean id="add4" class="Address">
		<property name="city" value="Pune"></property>
		<property name="state" value="Maharashtra"></property>

	</bean>


</beans>



